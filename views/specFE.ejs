<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="/css/style-nav.css">
    <link rel="stylesheet" href="/css/style-spesification.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <script src='https://kit.fontawesome.com/a076d05399.js'></script>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PLscore Spesifikasi</title>
</head>
<body>
    <!-- Navigation Bar -->
    <div class="navbar">
        <div class="logo"><img src="/img/logoepl2.png"><span><b>PL</b><em>score</em></span></div>
        <i class="material-icons" style="font-size:30px;color: whitesmoke;">view_headline</i>
        <!-- Desktop Nav -->
        <ul class="max-line">
            <li class="max-list"><a href="/"><i class="fa fa-home" style="font-size:26px;"></i></a></li>
            <li class="max-list"><a href="#">Berita</a></li>
            <li class="max-list"><a href="/pertandingan">Pertandingan</a></li>
            <li class="max-list actived"><a href="/spesifikasi">Spesifikasi</a></li>
            <li class="max-list"><a href="/akun"><i class="fa fa-user-circle" style="font-size:26px;"><span id="usename-account"></span></i></a></li>
        </ul>
        <!-- Android/Apple Nav -->
        <div class="for-js nav-menu">
            <ul class="min-line">
                <li class="min-list"><a href="/">Beranda</a></li>
                <li class="min-list"><a href="#">Berita</a></li>
                <li class="min-list"><a href="/pertandingan">Pertandingan</a></li>
                <li class="min-list actived"><a href="/spesifikasi">Spesifikasi</a></li>
                <li class="min-list"><a href="/akun"><i class="fa fa-user-circle" style="font-size:26px;"><span id="usename-account"></span></i></a></li>
            </ul>
        </div> 
    </div>

    <div class="sticky-backward display-none"><i class='fas fa-angle-left' style='font-size:40px; position: fixed; top: 20%; cursor: pointer; padding: 1px 2%;border-radius: 0 10px 10px 0; color: lightgray; background-color: #37003c;'></i></div>

    <!-- Title -->
    <h2>Tim-tim Liga Inggris</h2>
    <h2 class="display-none details-title"><span>Pemain-pemain</span> <strong></strong></h2>

    <!-- Search -->
    <div class="search-details">
        <input type="text" name="search_player" id="search-player" placeholder="Cari..." autocomplete="off">
        <div><button type="submit" id="playerSearch">Pemain</button></div>
        <div><button type="submit" id="positionSearch">Posisi</button></div>
        <div><button type="submit" id="nationalitySearch">Negara Asal</button></div>
    </div>

    <!-- Player Search Result -->
    <div class="player-search-result display-none">
        <span>X</span>
        <table></table>
        <div class="back">Kembali</div>
    </div>

    <!-- Teams Card -->
    <div class="draft"><div class="PLteams"></div></div>

    <!-- Players index -->
    <div class="players-index display-none">
        <div class="details">
            <table>
                <thead><tr><td style="width: 50%;" align="center"><strong>Nama</strong></td><td align="center" style="width: 30%;font: bolder;" class="column-detail-position"><strong>Position</strong></td><td class="position-column-for-nationality display-none" style="width: 20%;" align="center">Posisi</td></tr></thead>
                <tbody></tbody>
            </table>
        </div>
        <div class="back">Kembali</div>
        <div class="details-click display-none">
            <span>X</span>
            <table></table>
            <div class="back">Kembali</div>
        </div>
    </div>
    <div class="bg-disabled display-none"></div>

    <!-- Loading -->
    <div class="loading text-center display-none"></div>
    
    <!-- Footer -->
    <div class="footer">
        <span>PLscore</span><p>Copyright @ 2020 | PLscore Allright deserved | <a href="#">Contact</a></p>
    </div>


<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
<script src="/js/script.js"></script>
<script src="/js/script-spesification.js"></script>
</body>
</html>